{
  "name": "wasmd-indexer",
  "version": "1.0.0",
  "description": "",
  "license": "AGPL-3.0-only",
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "test": "docker compose -f compose.test.yml up --exit-code-from test",
    "test:computer": "docker compose -f compose.test.yml -f compose.test-computer.yml up --exit-code-from test",
    "jest": "NODE_ENV=test node --nolazy --inspect=0.0.0.0:9227 ./node_modules/.bin/jest --runInBand",
    "coverage": "npm run test -- --coverage",
    "prebuild": "rm -rf dist",
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently --raw \"tsc -w\" \"tsc-alias -w\"",
    "db:setup": "node dist/scripts/db/setup.js",
    "db:seed": "node dist/scripts/db/seed.js",
    "db:alter": "node dist/scripts/db/alter.js",
    "db:migrate:data": "MIGRATING_DB=true sequelize-cli db:migrate --env data",
    "db:migrate:accounts": "MIGRATING_DB=true sequelize-cli db:migrate --env accounts",
    "clear-cache": "node dist/scripts/clearCache.js",
    "export": "node dist/scripts/export/index.js",
    "export:dev": "docker-compose -f compose.dev.yml up --exit-code-from export",
    "export:prod": "npm run build && pm2 stop exporter webhooks && npm run db:migrate:data && pm2 reload ecosystem.config.js --only exporter,webhooks",
    "webhooks": "node dist/scripts/webhooks.js",
    "accountWebhooks": "node dist/scripts/accountWebhooks.js",
    "transform": "node dist/scripts/transform.js",
    "pre-compute": "node dist/scripts/preCompute.js",
    "revalidate": "node dist/scripts/revalidate.js",
    "flood": "node dist/scripts/flood.js",
    "serve": "node dist/server/serve.js",
    "serve:dev": "nodemon --watch dist --signal SIGTERM --exec \"node dist/server/serve.js\"",
    "serve:dev:accounts": "nodemon --watch dist --signal SIGTERM --exec \"node dist/server/serve.js -a\"",
    "serve:dev:testnet": "nodemon --watch dist --signal SIGTERM --exec \"node dist/server/serve.js -c config.testnet.json\"",
    "serve:dev:mainnet": "nodemon --watch dist --signal SIGTERM --exec \"node dist/server/serve.js -c config.mainnet.json\"",
    "serve:prod": "npm run build && pm2 reload ecosystem.config.js --only server_accounts,account-webhooks,server_juno-testnet,server_juno-mainnet",
    "serve:prod:accounts": "npm run build && pm2 reload ecosystem.config.js --only server_accounts",
    "deploy:server": "./scripts/deploy_server.sh",
    "deploy:server:accounts": "./scripts/deploy_server_accounts.sh",
    "deploy:exporter": "./scripts/deploy_exporter.sh",
    "search:update": "node dist/scripts/searchUpdate.js",
    "console": "node dist/scripts/console.js"
  },
  "author": "Noah Saso <noah@noah.zone>",
  "devDependencies": {
    "@cosmjs/proto-signing": "^0.29.5",
    "@shopify/jest-koa-mocks": "^5.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/koa": "^2.13.5",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.11.10",
    "@types/prettier": "^2.7.1",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "pm2": "^5.2.2",
    "prettier": "^2.8.0",
    "sequelize-cli": "^6.6.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.29.5",
    "@cosmjs/crypto": "^0.29.5",
    "@cosmjs/encoding": "^0.29.5",
    "@cosmjs/stargate": "^0.30.1",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@sentry/node": "^7.31.0",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^12.0.0",
    "axios": "^1.3.6",
    "commander": "^9.4.1",
    "crypto-js": "^4.1.1",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.13.4",
    "koa-body": "^6.0.1",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "meilisearch": "^0.30.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pusher": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.26.0",
    "sequelize-typescript": "^2.1.5",
    "supertest": "^6.3.3"
  }
}
